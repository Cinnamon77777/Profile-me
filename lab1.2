void GetSet(int[], int*);

int main() {
    int data[99], num;  
    GetSet(data, &num);

  
    printf("Values in the array: ");
    for (int i = 0; i < num; ++i) {
        printf("%d ", data[i]);
    }

    return 0;
}

void GetSet(int data[], int* num) {
    printf("Enter the number : ");
    scanf("%d", num);

   
    for (int i = 0; i < *num; ++i) {
        printf("Enter element %d: ", i + 1);
        scanf("%d", &data[i]);
    }
}
